   "source": [
    "import pandas as pd\n",
    "\n",
    "xls = pd.ExcelFile(\"Dataset for traffic analysis in Casablanca, Morocco.xlsx\")\n",
    "\n",
    "for sheet in xls.sheet_names:\n",
    "    df = pd.read_excel(xls, sheet_name=sheet)\n",
    "    df.to_csv(f\"{sheet}.csv\", index=False, encoding=\"utf-8\")"
   ]
  }
